@page "{id}"
@model MemberService.Pages.Payments.DetailsModel
@{
    Layout = null;
}
<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">Refunder @Model.Payment.Description</h5>
            <button type="button" class="close" data-close-dialog aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <p>Hvor mye skal refunderes?</p>

        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-close-dialog>Close</button>
        </div>
    </div>
</div>