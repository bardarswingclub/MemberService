@page "{id}"
@using MemberService.Data.ValueTypes
@using MemberService.Pages.Signup
@model EditModel

@{
    ViewData["Title"] = Model.SignupModel.Title;
    Layout = "~/Pages/_CoverLayout.cshtml";
}

<form method="POST" class="card shadow mt-5">
    <h3 class="card-header">
        @ViewData["Title"]
    </h3>
    <div class="card-body">
        <p class="lead">@Model.SignupModel.Description</p>

        <partial name="_SignupResponse" model=Model.SignupModel />
    </div>
    <div class="card-footer">
        <button type="submit" class="btn btn-success btn-block">Lagre endringer</button>
    </div>
</form>
