@model List<Event>

@{
    ViewData["Title"] = "Eventer";
}
<h2>@ViewData["Title"]</h2>

<a asp-action="Create">Opprett ny event</a>

<table class="table">
<thead>
    <tr>
        <th>Navn</th>
        <th>PÃ¥meldte</th>
    </tr>
</thead>
<tbody>
    @foreach(var eventEntry in Model)
    {
        <tr>
            <td><a asp-action="View" asp-route-id="@eventEntry.Id">@eventEntry.Title</a></td>
            <td>@eventEntry.Signups.Count</td>
        </tr>
    }
</tbody>
</table>